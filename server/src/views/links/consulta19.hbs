{{> navigation}}

<div class="container" style="color:darkred; margin-top: 20px;">
    <h5>CONSULTA 19</h5>
    <h5>
        Desplegar todos los países con el nombre de cada país con el cual tiene una
        frontera.
    </h5>
</div>

<div class="container pd-4" style="margin-top: 5px;">

    <table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">PAIS</th>
            <th scope="col">FRONTERA</th>
        </tr>
    </thead>
    <tbody>
        {{#each result}}
            <tr>            
                <th scope="row">{{pais}}</th>
                <td>{{frontera}}</td>
            </tr>
        {{/each}}
    </tbody>
    </table>

</div>