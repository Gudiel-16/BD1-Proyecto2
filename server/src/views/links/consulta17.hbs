{{> navigation}}

<div class="container" style="color:darkred; margin-top: 20px;">
    <h5>CONSULTA 17</h5>
    <h5>
        Desplegar el nombre de todos los inventos inventados el mismo año que BENZ
        invento algún invento.
    </h5>
</div>

<div class="container pd-4" style="margin-top: 5px;">

    <table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">INVENTOR</th>
            <th scope="col">INVENTO</th>
            <th scope="col">ANIO</th>
        </tr>
    </thead>
    <tbody>
        {{#each result}}
            <tr>            
                <th scope="row">{{inventor}}</th>
                <td>{{invento}}</td>
                <td>{{anio}}</td> 
            </tr>
        {{/each}}
    </tbody>
    </table>

</div>